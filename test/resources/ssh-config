  Hostname bupstashserver
  IdentitiesOnly yes
  IdentityFile /test/getsshkey
  StrictHostKeyChecking no

Host backup-list
  User bu
  Hostname bupstashserver
  IdentitiesOnly yes
  IdentityFile /test/listsshkey
  StrictHostKeyChecking no
